	<!-- Web clients i18n information -->	
    <string name="layout_transmission">Transmission</string>
	<string name="info_transmission">Warning! Transmission installed via CSI Installer \':9091/transmission/web/#files\' or \':9091/transmission/web\'. Default url \':8077/transmission/web\' should work if Transmission was installed by default in your NMT. Transmission is a bittorrent client runs natively on multiple operating systems.</string>
	<string name="info_transmission_homepage">http://transmissionbt.com</string>
	<string name="info_transmission_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
    
    <string name="layout_btcgi">BitTorrent</string> <!-- BTPD â€“ the bittorrent protocol daemon -->
	<string name="info_btcgi">BitTorrent (Btpd Client) (the BitTorrent Protocol Daemon Client) is a web client utility for sharing files over the BitTorrent network protocol.</string>
	<string name="info_btcgi_homepage">http://github.com/btpd/btpd/wiki</string>
	<string name="info_btcgi_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
        
    <string name="layout_nzbget">NZBGet</string> <!-- newsgrabber for nzb files -->
	<string name="info_nzbget">Warning! NZBget installed via CSI Installer \':9999/NZBget_web/\'. Default url \':8066/\' should work if NZBget was installed by default in your NMT. NZBGet is a Usenet Client newsgrabber for nzb files. It supports automatic par-check/-repair, unpack and has a web-interface. NZBGet requires low system resources and runs great on routers, NAS-devices and media players.</string>
	<string name="info_nzbget_homepage">http://nzbget.sourceforge.net</string>
	<string name="info_nzbget_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>

	<string name="layout_casgle">Casgle i-DVR</string>
	<string name="info_casgle">Casgle i-DVR simplifies the management of podcast content download to portable media player.</string>
	<string name="info_casgle_homepage">http://www.casgle.com</string>
	<string name="info_casgle_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
	<string name="layout_amule">aMule</string>
	<string name="info_amule">aMule is an eMule-like client for the eD2k and Kademlia networks, supporting multiple platforms.</string>
	<string name="info_amule_homepage">http://www.amule.org/index.php/Main_Page</string>
	<string name="info_amule_installation">http://www.networkedmediatank.com/showthread.php?tid=5609</string>
		
	<string name="layout_c200_remote">c200-remote</string>
	<string name="info_c200_remote">c200-remote allows send remote contol keys/actions to your nmt.</string>
	<string name="info_c200_remote_homepage">http://www.networkedmediatank.com/showthread.php?tid=13161</string>
	<string name="info_c200_remote_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
	<string name="layout_csi_gaya">CSI Gaya</string>
	<string name="info_csi_gaya">CSI Gaya allows to install software in your nmt via your browser. Install applications easily with this web interface.</string>
	<string name="info_csi_gaya_homepage">http://www.networkedmediatank.com/showthread.php?tid=13161</string>
	<string name="info_csi_gaya_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
	<string name="layout_download_manager">Download Manager</string>
	<string name="info_download_manager">Download Manager is a software to download files from sites like rapidshare.com or netload.in.</string>
	<string name="info_download_manager_homepage">http://networkedmediatank.com/showthread.php?tid=10592</string>
	<string name="info_download_manager_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
	<string name="layout_etvnet">eTVnet</string>
	<string name="info_etvnet">eTVnet allows to view the Russian TV archive stored by eTVnet provider.</string>
	<string name="info_etvnet_homepage">http://code.google.com/p/pch-etvnet</string>
	<string name="info_etvnet_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
	<string name="layout_feedtime">FeedTime</string>
	<string name="info_feedtime">FeedTime automatically download TV oriented NZB files from RSS and feed to nzbget.</string>
	<string name="info_feedtime_homepage">http://code.google.com/p/feedtime</string>
	<string name="info_feedtime_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>

	<string name="layout_filemanager">FileManager</string>
	<string name="info_filemanager">FileManager is a small file manager utility which will be helpful to browse the contents of PCH including the / directory.</string>
	<string name="info_filemanager_homepage">http://www.networkedmediatank.com/showthread.php?tid=17409</string>
	<string name="info_filemanager_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>

	<string name="layout_kartinatv">KartinaTV</string>
	<string name="info_kartinatv">KartinaTV allows to watch on Popcorn Hour family the IP-TV channels broadcast by Katrina.TV provider.</string>
	<string name="info_kartinatv_homepage">http://code.google.com/p/pch-kartinatv</string>
	<string name="info_kartinatv_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>

	<string name="layout_nmj_manager">NMJManager</string>
	<string name="info_nmj_manager">NMJManager is a web based editor for your NMJ Database.</string>
	<string name="info_nmj_manager_homepage">http://www.networkedmediatank.com/showthread.php?tid=46208</string>
	<string name="info_nmj_manager_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
	<string name="layout_oversight">Oversight</string>
	<string name="info_oversight">Oversight is a minimalistic Jukebox that doesn\'t need a PC. By default it shows the most recently downloaded content first.</string>
	<string name="info_oversight_homepage">http://code.google.com/p/oversight</string>
	<string name="info_oversight_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>

	<string name="layout_phpterm">phpterm</string>
	<string name="info_phpterm">PHPterm is a terminal/shell emulator for PHP which let you execute shell commands just like in a shell, but all within your browser window over the HTTP protocol. It aims to provide the look and feel of konsole, the KDE terminal.</string>
	<string name="info_phpterm_homepage">http://phpterm.sourceforge.net</string>
	<string name="info_phpterm_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
	<string name="layout_synk">SYNK</string>
	<string name="info_synk">SYNK is a graphical user interface / GUI for rsync. It has been built with PHP and javascript and runs over a web browser.</string>
	<string name="info_synk_homepage">http://www.networkedmediatank.com/showthread.php?tid=41963</string>
	<string name="info_synk_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
	<string name="layout_pwrc">Remote Control</string>	<!--Pure Web Remote Control PWRC-->
	<string name="info_pwrc">Web Remote Control provides NMT with an alternative/PDA based remote control to listen audio with TV switched off.</string>
	<string name="info_pwrc_homepage">http://sphinx-soft.com/tools/pwrc.html</string>
	<string name="info_pwrc_installation">http://www.networkedmediatank.com/wiki/index.php/Web_Remote_Control</string>
	
	<string name="layout_sysinfo">Sysinfo</string>
	<string name="info_sysinfo">Sysinfo is an extension for NMTs. This PHP-Script gives you an overview about your system stats, f.e. free disk space, uptime, ip-address, firmware-version, a.s.o.</string>
	<string name="info_sysinfo_homepage">http://www.hdd-player.de/syabas/showthread.php?tid=3574</string>
	<string name="info_sysinfo_installation">http://www.networkedmediatank.com/wiki/index.php/Sysinfo</string>
	
	<string name="layout_torrentwatch">TorrentWatch-X</string>
	<string name="info_torrentwatch">TorrentWatch-X is a web based tool to automate downloads of tv shows via torrents and rss. Based on the torrentwatch project (now known as NMTDVR).</string>
	<string name="info_torrentwatch_homepage">http://code.google.com/p/torrentwatch-x</string>
	<string name="info_torrentwatch_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>

	<string name="layout_mediatankcontroller">MediatankController</string>
	<string name="info_mediatankcontroller">MediatankController is completely free. Use the virtual remote or browse the contents of your NMT and directly start the file you want to play.</string>
	<string name="info_mediatankcontroller_homepage">http://www.mediatankcontroller.com</string>
	<string name="info_mediatankcontroller_installation">http://www.networkedmediatank.com/showthread.php?tid=20073</string>
	
	<string name="layout_nmtdvr">NMTDVR</string>
	<string name="info_nmtdvr">NMTDVR is an automated Tv Show and Movie Downloader that uses Usenet and Torrents.</string>
	<string name="info_nmtdvr_homepage">http://www.networkedmediatank.com/showthread.php?tid=38218</string>
	<string name="info_nmtdvr_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>

	<string name="layout_ipkg_web">IPKG web</string>
	<string name="info_ipkg_web">IPKG is a very lightweight package management system. It also allows for dynamic installation/removal of packages on a running system.</string>
	<string name="info_ipkg_web_homepage">http://www.networkedmediatank.com/showthread.php?tid=45424</string>
	<string name="info_ipkg_web_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>

	<string name="layout_couchpotato">Couchpotato</string>
	<string name="info_couchpotato">Couchpotato is an automatic NZB and torrent downloader. You can keep a "movies I want"-list and it will search for NZBs/torrents of these movies every X hours.</string>
	<string name="info_couchpotato_homepage">http://couchpotatoapp.com</string>
	<string name="info_couchpotato_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
	<string name="layout_musicbrowser">Music Browser</string>
	<string name="info_musicbrowser">Music Browser is a light-weight web-based browser and streamer for you music collection.</string>
	<string name="info_musicbrowser_homepage">http://musicbrowser.sf.net</string>
	<string name="info_musicbrowser_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
	<string name="layout_sickbeard">Sick Beard</string>
	<string name="info_sickbeard">Sick Beard is a PVR application that downloads and manages your TV shows.</string>
	<string name="info_sickbeard_homepage">http://sickbeard.com</string>
	<string name="info_sickbeard_installation">Install this application in your Networked Media Tank using NMT Community Software Installer</string>
	
