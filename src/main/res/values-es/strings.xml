	<!-- Web clients i18n information -->	
    <string name="layout_transmission">Transmission</string>
	<string name="info_transmission">Aviso! Transmission instalado via CSI Installer \':9091/transmission/web/#files\' o \':9091/transmission/web\'. La url por defecto \':8077/transmission/web\' si Transmission viene instalado por defecto en tu NMT. Transmission es un cliente bittorrent que se ejecuta de forma nativa en múltiples sistemas operativos.</string>
	<string name="info_transmission_homepage">http://transmissionbt.com</string>
	<string name="info_transmission_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
    <string name="layout_btcgi">BitTorrent</string> <!-- BTPD – the bittorrent protocol daemon -->
	<string name="info_btcgi">BitTorrent Cliente (Cliente Demonio para protocolo BitTorrent) es una utilidad cliente web para compartir ficheros sobre el protoclo de red BitTorrent.</string>
	<string name="info_btcgi_homepage">http://github.com/btpd/btpd/wiki</string>
	<string name="info_btcgi_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
    <string name="layout_nzbget">NZBGet</string> <!-- newsgrabber for nzb files -->
	<string name="info_nzbget">Warning! NZBget instalado via CSI Installer \':9999/NZBget_web/\'. La url por defecto \':8066/\' si NZBget viene instalado por defecto en tu NMT. NZBGet es un cliente de Usenet para ficheros nzb. Soporta comprobación/reparación de pares, descompresión y tiene un interfaz web. NZBGet requiere pocos recursos del sistema y funciona muy bien en routers, unidades NAS y reproductores multimedia.</string>
	<string name="info_nzbget_homepage">http://nzbget.sourceforge.net</string>
	<string name="info_nzbget_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
	<string name="layout_casgle">Casgle i-DVR</string>
	<string name="info_casgle">Casgle i-DVR simplifica la descarga de contenido podcast para reproductores multimedia.</string>
	<string name="info_casgle_homepage">http://www.casgle.com</string>
	<string name="info_casgle_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>

	<string name="layout_amule">aMule</string>
	<string name="info_amule">aMule es un cliente de tipo eMule para las redes eD2k y Kademlia, soporta multiplataforma.</string>
	<string name="info_amule_homepage">http://www.amule.org/index.php/Main_Page</string>
	<string name="info_amule_installation">http://www.networkedmediatank.com/showthread.php?tid=5609</string>

	<string name="layout_c200_remote">c200-remote</string>
	<string name="info_c200_remote">c200-remote permite enviar teclas de control remotas a tu NMT.</string>
	<string name="info_c200_remote_homepage">http://www.networkedmediatank.com/showthread.php?tid=13161</string>
	<string name="info_c200_remote_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
	<string name="layout_csi_gaya">CSI Gaya</string>
	<string name="info_csi_gaya">CSI Gaya permite instalar sofware en tu NMT usando tu navegador web. Instala aplicaciones de forma fácil con este interfaz web.</string>
	<string name="info_csi_gaya_homepage">http://www.networkedmediatank.com/showthread.php?tid=13161</string>
	<string name="info_csi_gaya_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
	<string name="layout_download_manager">Download Manager</string>
	<string name="info_download_manager">Download Manager permite descargar ficheros desde sitios como rapidshare.com o netload.in.</string>
	<string name="info_download_manager_homepage">http://networkedmediatank.com/showthread.php?tid=10592</string>
	<string name="info_download_manager_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
	<string name="layout_etvnet">eTVnet</string>
	<string name="info_etvnet">eTVnet permite ver archivo de Tv ruso almacenado por el el proveedor eTVnet.</string>
	<string name="info_etvnet_homepage">http://code.google.com/p/pch-etvnet</string>
	<string name="info_etvnet_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
	<string name="layout_feedtime">FeedTime</string>
	<string name="info_feedtime">FeedTime descargar automáticamente archivos de TV orientada NZB de RSS y de alimentación a nzbget.</string>
	<string name="info_feedtime_homepage">http://code.google.com/p/feedtime</string>
	<string name="info_feedtime_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
	<string name="layout_filemanager">FileManager</string>
	<string name="info_filemanager">FileManager es una utilidad de administrador de archivos pequeños, que será útil para examinar el contenido de PCH incluido el directorio /.</string>
	<string name="info_filemanager_homepage">http://www.networkedmediatank.com/showthread.php?tid=17409</string>
	<string name="info_filemanager_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
	<string name="layout_kartinatv">KartinaTV</string>
	<string name="info_kartinatv">KartinaTV permite ver en los dispositivos de la familia Popcorn Hour los canales de televisión IP del proveedor de Katrina.TV.</string>
	<string name="info_kartinatv_homepage">http://code.google.com/p/pch-kartinatv</string>
	<string name="info_kartinatv_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>

	<string name="layout_nmj_manager">NMJManager</string>
	<string name="info_nmj_manager">NMJManager es un editor basado en web para tu NMJ Database.</string>
	<string name="info_nmj_manager_homepage">http://www.networkedmediatank.com/showthread.php?tid=46208</string>
	<string name="info_nmj_manager_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>

	<string name="layout_oversight">Oversight</string>
	<string name="info_oversight">Oversight es un minimalista Jukebox que no necesita un PC. Por defecto se muestra el contenido descargado más recientemente primero.</string>
	<string name="info_oversight_homepage">http://code.google.com/p/oversight</string>
	<string name="info_oversight_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>

	<string name="layout_phpterm">phpterm</string>
	<string name="info_phpterm">PHPterm es un emulador de terminal para PHP que permite ejecutar comandos de terminal como en un terminal, pero todo dentro de una ventana de navegador sobre el protocolo HTTP. Su objetivo es proporcionar la apariencia de la consola, el terminal de KDE.</string>
	<string name="info_phpterm_homepage">http://phpterm.sourceforge.net</string>
	<string name="info_phpterm_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
	<string name="layout_synk">SYNK</string>
	<string name="info_synk">SYNK es un interfaz gráfico de usuario / GUI para rsync. Ha sido construido con PHP y javascript y se ejecuta en un navegador web.</string>
	<string name="info_synk_homepage">http://www.networkedmediatank.com/showthread.php?tid=41963</string>
	<string name="info_synk_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>

	<string name="layout_pwrc">Remote Control</string> <!-- Pure Web Remote Control PWRC-->
	<string name="info_pwrc">Web Remote Control provee a NMT con una alternativa de control remoto para escuchar audio con el televisor apagado.</string>
	<string name="info_pwrc_homepage">http://sphinx-soft.com/tools/pwrc.html</string>
	<string name="info_pwrc_installation">http://www.networkedmediatank.com/wiki/index.php/Web_Remote_Control</string>

	<string name="layout_sysinfo">Sysinfo</string>
	<string name="info_sysinfo">Sysinfo es una extensión para NMTs. Este script PHP ofrece una visión general sobre estadísticas de su sistema, p.e. espacio libre en disco, tiempo de actividad, dirección ip, versión del firmware, y otras.</string>
	<string name="info_sysinfo_homepage">http://www.hdd-player.de/syabas/showthread.php?tid=3574</string>
	<string name="info_sysinfo_installation">http://www.networkedmediatank.com/wiki/index.php/Sysinfo</string>

	<string name="layout_torrentwatch">TorrentWatch-X</string>
	<string name="info_torrentwatch">TorrentWatch-X es una herramienta basada en web que permite automatizar las descargas de programas de televisión a través de torrents y RSS. Basado en el proyecto torrentwatch (ahora conocido como NMTDVR).</string>
	<string name="info_torrentwatch_homepage">http://code.google.com/p/torrentwatch-x</string>
	<string name="info_torrentwatch_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>

	<string name="layout_mediatankcontroller">MediatankController</string>
	<string name="info_mediatankcontroller">MediatankController es completamente gratuito. Usa el control remoto virtual o navega el contenido de tu NMT y reproduce directamente el fichero que quieras.</string>
	<string name="info_mediatankcontroller_homepage">http://www.mediatankcontroller.com</string>
	<string name="info_mediatankcontroller_installation">http://www.networkedmediatank.com/showthread.php?tid=20073</string>

	<string name="layout_nmtdvr">NMTDVR</string>
	<string name="info_nmtdvr">NMTDVR es un Descargador automático de Shows de TV y Películas que usa Usenet y Torrents.</string>
	<string name="info_nmtdvr_homepage">http://www.networkedmediatank.com/showthread.php?tid=38218</string>
	<string name="info_nmtdvr_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
	<string name="layout_ipkg_web">IPKG web</string>
	<string name="info_ipkg_web">IPKG es un ligero sistema de administración de paquetes. Permite la instalación/desinstalación dinámica de paquetes sobre el sistema operativo.</string>
	<string name="info_ipkg_web_homepage">http://www.networkedmediatank.com/showthread.php?tid=45424</string>
	<string name="info_ipkg_web_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>

	<string name="layout_couchpotato">Couchpotato</string>
	<string name="info_couchpotato">Couchpotato es un descargador automático de NZB y torrent. Puedes mantener una lista de peliculas que quieres ver y buscará NZBs/torrents de esas películas cada X horas.</string>
	<string name="info_couchpotato_homepage">http://couchpotatoapp.com</string>
	<string name="info_couchpotato_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>

	<string name="layout_musicbrowser">Music Browser</string>
	<string name="info_musicbrowser">Music Browser es un ligero navegador y streamer para tu colección de música.</string>
	<string name="info_musicbrowser_homepage">http://musicbrowser.sf.net</string>
	<string name="info_musicbrowser_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>

	<string name="layout_sickbeard">Sick Beard</string>
	<string name="info_sickbeard">Sick Beard es una aplicación PVR que descarga y administra series de TV.</string>
	<string name="info_sickbeard_homepage">http://sickbeard.com</string>
	<string name="info_sickbeard_installation">Instala esta aplicación en tu Networked Media Tank usando NMT Community Software Installer</string>
	
